<%= render 'devise/shared/profile_breadcrumb' %>
<%=
  current_user.inspect
%>
<div class="personal">
  <div class="zyxel_content" style="max-width: 530px;">
    <% if flash[:notice] %>
      <div class="zyxel_smallok_area">
        <span><%= flash[:notice] %></span>
      </div>
    <% end %>
    <div class="zyxel_edm">
      <%= check_box_tag "edm_accept", value = current_user.edm_accept, !current_user.edm_accept.zero?, disabled: true %>
      <label><span></span><%= I18n.t("user.labels.edm") %></label>
    </div>

    <div class="zyxel_btn_area">
      <% if !session[:oauth] %>
        <%= link_to I18n.t("labels.change_password"), edit_user_registration_path(type: "password"), class: "zyxel_btn_ok zyxel_btn_ok1" %>
      <% end %>
      <%= link_to I18n.t("labels.edit_profile"), edit_user_registration_path(type: "profile"), class: "zyxel_btn_ok" %>
    </div><!-- zyxel_btn_area --> 

  </div><!-- content -->
</div>